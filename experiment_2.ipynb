    "from langchain.chains import LLMChain\n",
    "import ast\n",
    "\n",
    "def cv_summary(extracted_text):\n",
    "    chain = LLMChain(llm=llm, prompt=prompt_template)\n",
    "    summarized_text = chain.run(extracted_text=extracted_text)\n",
    "    return summarized_text"
   ]
